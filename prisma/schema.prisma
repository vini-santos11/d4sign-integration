generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model PDF {
  id                 Int     @id @default(autoincrement())
  id_d4sign          String
  name               String
  url                String
  already_downloaded Boolean
}
